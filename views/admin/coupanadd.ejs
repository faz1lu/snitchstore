			<div class="ec-content-wrapper">
				<div class="content">
					<div class="breadcrumb-wrapper breadcrumb-wrapper-2 breadcrumb-contacts">
						<h1>add coupan</h1>
						<p class="breadcrumbs"><span><a href="/admin">Home</a></span>
							<span><i class="mdi mdi-chevron-right"></i></span>coupan add</p>
					</div>
					<div class="row">
						<div class="col-xl-4 col-lg-12">
							<div class="ec-cat-list card card-default mb-24px">
								<div class="card-body">
									<div class="ec-cat-form">
										<h4>Add  Coupan</h4>

										<form method="post" action="/admin/coupanaddPost">

                                            <div class="form-group row">
												<label for="text" class="col-12 col-form-label">Coupan code</label> 
												<div class="col-12">
													<input id="text" name="coupanname" class="form-control here slug-title" type="text" placeholder="coupan code">
												</div>
											</div>

											<div class="form-group row">
												<label for="text" class="col-12 col-form-label">Min.Redeem Amount</label> 
												<div class="col-12">
													<input id="text" name="redeem" class="form-control here slug-title" type="text" placeholder="Min.Redeem Amount">
												</div>
											</div>
											<div class="form-group row">
												<label for="text" class="col-12 col-form-label">available</label> 
												<div class="col-12">
													<input id="text" name="available" class="form-control here slug-title" type="text" placeholder="available">
												</div>
											</div>
											<div class="form-group row">
												<label for="text" class="col-12 col-form-label">min-cart amount</label> 
												<div class="col-12">
													<input id="text" name="mincartAmount" class="form-control here slug-title" type="text" placeholder="cart amount">
												</div>
											</div>
											<div class="form-group row">
												<label for="text" class="col-12 col-form-label">start date</label> 
												<div class="col-12">
													<input id="text" name="startdate" class="form-control here slug-title" type="date" placeholder="start date">
												</div>
											</div>
											<div class="form-group row">
												<label for="text" class="col-12 col-form-label">end date</label> 
												<div class="col-12">
													<input id="text" name="enddate" class="form-control here slug-title" type="date" placeholder="end date">
												</div>
											</div>

											

											

											<div class="row">
												<div class="col-12">
													<button name="submit" type="submit" class="btn btn-primary">Submit</button>
												</div>
											</div>

										</form>

									</div>
								</div>
							</div>
						</div>
						<div class="col-xl-8 col-lg-12">
							<div class="ec-cat-list card card-default">
								<div class="card-body">
                                    <form action="/admin/categorydisplay">
									<div class="table-responsive">
										<table id="responsive-data-table" class="table">
											
                                            <thead>
											<tr>
												<th>No</th>
												<th>Coupan code</th>
												<th>coupan value</th>
												<th>start date</th>
												<th>expiry date</th>
												<th>min-cartamount</th>
												<!-- <th>Max amount</th>  -->
												<!-- <th>usage limit</th> -->
												<th>status</th>
												<th>available</th>
												<th>action</th>
												
												
											</tr>
												
											</thead>

											<tbody>
                                                <% for( let i=0 ; i<coupanDetails.length ; i++) { %> 
                                                    <tr>
                                                         <td><%= i+1 %> </td>
                                                        <td><%= coupanDetails[i].code %> </td>
                                                        <td><%= coupanDetails[i].redeemamount %> </td> 
														<td><%= coupanDetails[i].startdate%></td>
														<td><%=coupanDetails[i].validupto%></td>
														<td><%= coupanDetails[i].mincartAmount%></td>
														<td><%=coupanDetails[i].status%></td>
														<td><%= coupanDetails[i].available%></td>
														<td>
															<!-- <a href="/admin/coupandelete/<= coupanDetails[i].id %> "> <button type="button" class="btn btn-danger" >delete</button></a> -->
															<a onclick="coupandelete('<%=coupanDetails[i].id%>')"><i class="fa-solid fa-trash" style="color: brown;"></i></a>
														</td>	
														
                                                        <!-- <td>
                                                            <span class="ec-sub-cat-list">
