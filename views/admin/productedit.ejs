<body class="ec-header-fixed ec-sidebar-fixed ec-sidebar-dark ec-header-light" id="body">

  <!-- WRAPPER -->
  <div class="wrapper">

    <!-- LEFT MAIN SIDEBAR -->
    <%-include('partials/mainheader')  %>
    <!-- PAGE WRAPPER -->
    <div class="ec-page-wrapper">

      <!-- Header -->
      <%-include('partials/sidebardashboard.ejs')  %>
      <!-- CONTENT WRAPPER -->
      <div class="ec-content-wrapper">
        <div class="content">
          <div class="breadcrumb-wrapper d-flex align-items-center justify-content-between">
            <div>
              <h1>Edit Product</h1>
              <p class="breadcrumbs"><span><a href="index.html">Home</a></span>
                <span><i class="mdi mdi-chevron-right"></i></span>Product
              </p>
            </div>
            <div>
              <a href="/admin/productlist" class="btn btn-primary"> View All
              </a>
            </div>
          </div>
          <div class="row">
            <div class="col-12">
              <div class="card card-default">
                <div class="card-header card-header-border-bottom">
                  <h2>Add Product</h2>
                </div>

                <div class="card-body">

					
                  <form class="row g-3" method="post" action="/admin/editing/<%= findedit.id %> " enctype="multipart/form-data">
                    <div class="row ec-vendor-uploads">
                      <div class="col-lg-4">
                        <div class="ec-vendor-img-upload">
                          <div class="ec-vendor-main-img">
                            <div class="avatar-upload">
                              <div class="avatar-edit">
                                <input type='file' name="image" id="imageUpload" class="ec-image-upload" accept=".png, .jpg, .jpeg" />
                                <label for="imageUpload"><img src="/admin/img/icons/edit.svg" class="svg_img header_svg" alt="edit" /></label>
                              </div>
                              <div class="avatar-preview ec-preview">
                                <div class="imagePreview ec-div-preview">
                                  <img class="ec-image-preview" src="<%= findedit.image[0] %>" alt="edit" />
                                </div>
                              </div>
                            </div>
                            <div class="thumb-upload-set colo-md-12">
                              <div class="thumb-upload">
                                <div class="thumb-edit">
                                  <input type='file' name="image" id="thumbUpload01" class="ec-image-upload" accept=".png, .jpg, .jpeg" />
                                  <label for="imageUpload"><img src="/admin/img/icons/edit.svg" class="svg_img header_svg" alt="edit" /></label>
                                </div>
                                <div class="thumb-preview ec-preview">
                                  <div class="image-thumb-preview">
                                    <img class="image-thumb-preview ec-image-preview" src="<%= findedit.image[1] %>" alt="edit" />
                                  </div>
                                </div>
                              </div>
                              <div class="thumb-upload">
                                <div class="thumb-edit">
                                  <input type='file' name="image" id="thumbUpload02" class="ec-image-upload" accept=".png, .jpg, .jpeg" />
                                  <label for="imageUpload"><img src="/admin/img/icons/edit.svg" class="svg_img header_svg" alt="edit" /></label>
                                </div>
                                <div class="thumb-preview ec-preview">
                                  <div class="image-thumb-preview">
                                    <img class="image-thumb-preview ec-image-preview" src="<%= findedit.image[2] %>" alt="edit" />
                                  </div>
                                </div>
                              </div>
                              <div class="thumb-upload">
                                <div class="thumb-edit">
                                  <input type='file' name="image" id="thumbUpload03" class="ec-image-upload" accept=".png, .jpg, .jpeg" />
                                  <label for="imageUpload"><img src="/admin/img/icons/edit.svg" class="svg_img header_svg" alt="edit" /></label>
                                </div>
                                <div class="thumb-preview ec-preview">
                                  <div class="image-thumb-preview">
                                    <img class="image-thumb-preview ec-image-preview" src="<%= findedit.image[3] %>" alt="edit" />
                                  </div>
                                </div>
                              </div>

                            </div>
                          </div>
                        </div>
                      </div>
                      <div class="col-lg-8">
                        <div class="ec-vendor-upload-detail">

                          <div class="col-md-12">
                            <label for="inputEmail4" class="form-label">Product name</label>
                            <input type="text" name="name" class="form-control " id="inputEmail4" type="name" value="<%= findedit.name %> ">
                          </div>
                          <div class="col-md-12">
                            <label class="form-label">Select Categories</label>
                                       
                            <select id="Categories" class="form-select" >
                              <optgroup label="Fashion">
                                <% for(let i=0;i<cats.length;i++) { %> 
                                <option ><%=cats[i].name  %> </option>
                                <% } %> 
                              </optgroup>
                            
                            </select>
                          </div>
                          <div class="col-md-12">
                            <label for="" class="col-12 col-form-label">Brand</label>
                            <div class="col-12">
                              <input id="slug" name="brand" class="form-control " type="text" value="<%= findedit.brand %> ">
                            </div>
                          </div>
                          <div class="col-md-12">
                            <label class="form-label" name="description"> Description</label>
                            <input class="form-control" rows="2" value="<%=  findedit.description%>" name="description" />
                          </div>
                          <div class="col-md-12">
                            <label class="form-label"> select colour</label>
                            <input class="form-control" rows="2" name="selectcolor" value="<%=   findedit.selectcolor%>">
                          </div>
                          <!-- <div class="col-md-4 mb-25">
														<label class="form-label">Colors</label>
														<input type="color" class="form-control form-control-color"
															id="exampleColorInput1" value="#ff6191"
															title="Choose your color">
														<input type="color" class="form-control form-control-color"
															id="exampleColorInput2" value="#33317d"
															title="Choose your color">
														<input type="color" class="form-control form-control-color"
															id="exampleColorInput3" value="#56d4b7"
															title="Choose your color">
														<input type="color" class="form-control form-control-color"
															id="exampleColorInput4" value="#009688"
															title="Choose your color">
													</div> -->
                          <div class="col-md-8 mb-25">
                            <label class="form-label">Size</label>
                            <div class="form-checkbox-box">
                              <div class="form-check form-check-inline">
                                <input type="checkbox"  value="s">
                                <label name="size">S</label>
                              </div>
                              <div class="form-check form-check-inline">
                                <input type="checkbox" name="size" value="M">
                                <label value="M">M</label>
                              </div>
                              <div class="form-check form-check-inline">
                                <input type="checkbox" name="size" value="L">
                                <label name="size">L</label>
                              </div>
                              <div class="form-check form-check-inline">
                                <input type="checkbox" name="size" value="XL">
                                <label>XL</label>
                              </div>
                              <div class="form-check form-check-inline">
                                <input type="checkbox" name="size" value="XXL">
                                <label>XXL</label>
                              </div>
                            </div>
                          </div>
                          <div class="col-md-12">
                            <label class="form-label">Price</label>

                            <input type="number" name="price" class="form-control" id="price1" value="<%=  findedit.price %>">
                          </div>
                          <div class="col-md-12">
                            <label class="form-label">Quantity</label>
                            <input type="number" name="quantity" class="form-control" id="quantity1" value="<%=  findedit.quantity %>">
                          </div>
                          <div class="col-md-12">
                            <label class="form-label" name="purchased"> purchased</label>
                            <input class="form-control" rows="2" value="<%=  findedit.purchased%>" name="purchased" />
                          </div>
						              <!-- <div class="col-md-12">
                            <label class="form-label">Purchased</label>
                            <input type="number" name="purchased" class="form-control" id="quantity1" value="purchased" >
                          </div> -->
                          <div class="col-md-12">
                            <label class="form-label" name="discount"> Discount</label>
                            <input class="form-control" rows="2" value="<%=  findedit.discount%>" name="discount" />
                          </div>
						                <!-- <div class="col-md-12">
                            <label class="form-label">Discount</label>
                            <input type="number" name="discount" class="form-control" id="quantity1" value="">
                          </div> -->
                          <!-- <div class="col-md-12">
														<label class="form-label">Ful Detail</label>
														<textarea class="form-control" rows="4"></textarea>
													</div> -->
                          <!-- <div class="col-md-12">
														<label class="form-label">Product Tags <span>( Type and
																make comma to separate tags )</span></label>
														<input type="text" class="form-control" id="group_tag"
															name="group_tag" value="" placeholder=""
															data-role="tagsinput" />
													</div> -->
                          <div class="col-md-12 m-3">
                            <button type="submit" class="btn btn-primary" href="/admin/editing">submit</button>
                          </div>

                        </div>
                      </div>
                    </div>  
                    <!-- jjjj -->
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div> <!-- End Content -->
      </div> <!-- End Content Wrapper -->

      <!-- Footer -->
      <footer class="footer mt-auto">
        <div class="copyright bg-white">
          <p>
            Copyright &copy; <span id="ec-year"></span><a class="text-primary" href="https://themeforest.net/admin/ashishmaraviya" target="_blank"> Ekka Admin Dashboard</a>. All Rights Reserved.
          </p>
        </div>
      </footer>

    </div> <!-- End Page Wrapper -->
  </div> <!-- End Wrapper -->

  <!-- Common Javascript -->
  <script src="/admin/plugins/jquery/jquery-3.5.1.min.js"></script>
  <script src="/admin/js/bootstrap.bundle.min.js"></script>
  <script src="/admin/plugins/tags-input/bootstrap-tagsinput.js"></script>
  <script src="/admin/plugins/simplebar/simplebar.min.js"></script>
  <script src="/admin/plugins/jquery-zoom/jquery.zoom.min.js"></script>
  <script src="/admin/plugins/slick/slick.min.js"></script>

  <!-- Option Switcher -->
  <script src="/admin/plugins/options-sidebar/optionswitcher.js"></script>

  <!-- Ekka Custom -->
  <script src="/admin/js/ekka.js"></script>
</body>


